@using MudBlazor
<div class="pb-6">
    <PageTitle>@Title</PageTitle>
    <MudText Typo="Typo.h5" Color="Color.Default" Class="mb-2">@Header</MudText>
    <MudText Typo="Typo.body1" Color="Color.Default" Class="mb-2">@SubHeader</MudText>
</div >
@code {
    [Parameter]
    public required string Title { get; set; }

    [Parameter]
    public required string Header { get; set; }

    [Parameter]
    public required string SubHeader { get; set; }
}
